# your code goes here
x,y = map(int,input().split())
a = []
b = []
for i in range(x):
    t = list(map(int,input().split()))
    a.append(t)
dum = input()
for i in range(x):
    t = list(map(int,input().split()))
    b.append(t)
    
c = [[0 for _ in range(y)] for _ in range(x)]

for i in range(x):
    for j in range(y):
        c[i][j] = a[i][j]+b[i][j]

for i in range(x):
    for j in range(y):
        print(c[i][j],end =" ")
    print()

    
